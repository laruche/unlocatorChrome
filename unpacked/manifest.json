{
  "name": "Unlocator Status",

  "description": "Update your IP configuration automatically !",
  "author" : "laruche",
  "version": "0.1.5",
  "permissions": [
    "webRequest","tabs", "storage"
  ],
  "icons": { "128": "icon.png" },
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
      "default_title": "Unlocator extension",
      "default_icon": "icon.png",
      "default_popup": "html/popupLogin.html"
  },  
  "manifest_version": 2
}
